<p id="notice"><%= notice %></p>

<h1>Gmails</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Gmailemail</th>
      <th>Gmailpass</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gmails.each do |gmail| %>
      <tr>
        <td><%= gmail.email %></td>
        <td><%= gmail.gmailemail %></td>
        <td><%= gmail.gmailpass %></td>
        <td><%= link_to 'Show', gmail %></td>
        <td><%= link_to 'Edit', edit_gmail_path(gmail) %></td>
        <td><%= link_to 'Destroy', gmail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gmail', new_gmail_path %>
