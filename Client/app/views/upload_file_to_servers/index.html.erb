<p id="notice"><%= notice %></p>

<h1>Upload File To Servers</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Filename</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_file_to_servers.each do |upload_file_to_server| %>
      <tr>
        <td><%= upload_file_to_server.email %></td>
        <td><%= upload_file_to_server.filename %></td>
        <td><%= link_to 'Show', upload_file_to_server %></td>
        <td><%= link_to 'Edit', edit_upload_file_to_server_path(upload_file_to_server) %></td>
        <td><%= link_to 'Destroy', upload_file_to_server, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload File To Server', new_upload_file_to_server_path %>
